    <string name="Ours_ex"><b><u>Eng</u> : This place is OURS.\n<u>Kan</u> : ee jaaga NAMMADU.</b></string>
    <string name="You_Singular_ex"><b><u>Eng</u> : YOU are my friend.\n<u>Kan</u> : NEENU nanna gelaya.</b></string>
