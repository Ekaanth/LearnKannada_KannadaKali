    <string name="I_ex"><b><u>Eng</u> : I am Varun.</b>\n<b><u>Kan</u> : Naanu Varun.</b></string>
    <string name="We_ex"><b><u>Eng</u> : WE are Indians.\n<u>Kan</u> : NAAVU bhaaratheeyaru.</b></string>
    <string name="My_ex"><b><u>Eng</u> : MY name is Rohith.\n<u>Kan</u> : NANNA hesaru Rohith.</b></string>
    <string name="Our_ex"><b><u>Eng</u> : OUR country is India.\n<u>Kan</u> : NANNA hesaru Rohith.</b></string>
    <string name="For_Us_ex"><b><u>Eng</u> : This is FOR US.\n<u>Kan</u> : idu NAMAGE.</b></string>
    <string name="For_Me_ex"><b><u>Eng</u> : This is FOR ME.\n<u>Kan</u> : idu NANAGE.</b></string>
    <string name="Mine_ex"><b><u>Eng</u> : This pen is MINE.\n<u>Kan</u> : ee pen NANNADU.</b></string>
    <string name="Ours_ex"><b><u>Eng</u> : This place is OURS.\n<u>Kan</u> : ee jaaga NAMMADU.</b></string>
    <string name="You_Singular_ex"><b><u>Eng</u> : YOU are my friend.\n<u>Kan</u> : NEENU nanna gelaya.</b></string>
    <string name="You_Plural_ex"><b><u>Eng</u> : YOU are my teacher.\n<u>Kan</u> : NEEVU nanna guru.</b></string>
    <string name="Your_Singular_ex"><b><u>Eng</u> : This is YOUR book.\n<u>Kan</u> : idu NINNA pusthaka.</b></string>
    <string name="Your_Plural_ex"><b><u>Eng</u> : These are YOUR books.\n<u>Kan</u> : ivu NIMMA pusthakagalu.</b></string>
    <string name="For_You_Singular_ex"><b><u>Eng</u> : This book is FOR YOU.\n<u>Kan</u> : ee pusthaka NINAGE.</b></string>
    <string name="For_You_Plural_ex"><b><u>Eng</u> : These books are FOR YOU.\n<u>Kan</u> : ee pusthakagalu NIMAGE.</b></string>
    <string name="Yours_Singular_ex"><b><u>Eng</u> : This phone is YOURS.\n<u>Kan</u> : ee phone NINNADU.</b></string>
    <string name="Yours_Plural_ex"><b><u>Eng</u> : This place is YOURS.\n<u>Kan</u> : ee jaaga NIMMADU.</b></string>
